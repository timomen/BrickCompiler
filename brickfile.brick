// system declarators
!commandline
---
// functions or things that don't run immediately
functionname.argument1.argument2 {
    back argument1 + argument2;
}
---
// variable setup
varname; var2name; // only declerates
var3name."string with spaces"; // declerates and sets
---
// code that runs on startup
cl.out(functionname.(5+2).var2name);
while.(var2name == 5) {
    cl.in("Enter age: ".var2name);
    if.(var2name<42){
        cl.out.var2name;
    }
}
